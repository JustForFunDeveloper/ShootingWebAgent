{
  "ConnectionStrings": {
    "DataDbConnection": "DataSource=Data.db",
    "IdentityContextConnection": "Data Source=ShootingWebAgent.db"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log_.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "14"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*"
}